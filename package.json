    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "prettier": "^2.5.1",
    "rollup": "^2.67.2",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-typescript2": "^0.31.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "fetch-npm-package": "^1.0.15",
