{
  "name": "kaisa",
  "version": "0.0.1",
  "description": "Runtime npm package loader",
  "main": "dist/kaisa.cjs.js",
  "module": "dist/kaisa.esm-bundler.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "build": "rollup --config",
    "dev": "http-server ./",
