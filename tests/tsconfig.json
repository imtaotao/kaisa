{
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "dist",
    "module": "ESNext",
    "target": "es6",
    "strict": true,
    "resolveJsonModule": true,
    "lib": ["es6", "dom"],
    "moduleResolution": "node",
    "allowJs": true,
    "sourceMap": true,
    "declaration": false,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "paths": {
      "kaisa": ["../src/index.ts"]
    }
  },
  "include": [
    "scripts"
  ],
  "exclude": [
    "node_modules"
  ]
}
